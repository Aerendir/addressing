{
    "locale": "es",
    "format": "%recipient\n%organization\n%address\n%locality %postal_code, %administrative_area",
    "required_fields": [
        "address",
        "locality",
        "administrative_area"
    ],
    "uppercase_fields": [
        "locality",
        "administrative_area"
    ],
    "administrative_area_type": "province",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{4}"
}