{
    "locale": "en",
    "format": "%organization\n%recipient\n%address\n%locality %administrative_area %postal_code",
    "required_fields": [
        "address",
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality",
        "administrative_area"
    ],
    "administrative_area_type": "state",
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{4}"
}