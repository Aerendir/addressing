{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%postal_code %locality",
    "required_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "province",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{2}-\\d{3}"
}