{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%postal_code\n%locality",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "locality",
        "postal_code"
    ],
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "(?:[A-Z]\\d{4}[A-Z]|(?:[A-Z]{2})?\\d{6})?"
}