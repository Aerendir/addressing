{
    "locale": "zh-Latn",
    "format": "%recipient\n%organization\n%address\n%locality, %administrative_area %postal_code",
    "required_fields": [
        "address",
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "county",
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{3}(\\d{2})?",
    "translations": {
        "zh-hant": {
            "format": "%postal_code\n%administrative_area%locality\n%address\n%organization\n%recipient"
        }
    }
}