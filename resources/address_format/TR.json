{
    "locale": "tr",
    "format": "%recipient\n%organization\n%address\n%postal_code %locality\/%administrative_area",
    "required_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "province",
    "locality_type": "district",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{5}"
}