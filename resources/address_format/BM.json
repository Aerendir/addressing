{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%locality %postal_code",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "province",
    "postal_code_type": "postal",
    "postal_code_pattern": "[A-Z]{2}[ ]?[A-Z0-9]{2}"
}