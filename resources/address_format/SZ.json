{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%locality\n%postal_code",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "administrative_area_type": "province",
    "postal_code_type": "postal",
    "postal_code_pattern": "[HLMS]\\d{3}"
}