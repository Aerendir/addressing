{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%locality %postal_code",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "(?:(?:\\d|1[0-2])\\d{2})?"
}