{
    "locale": "ca",
    "format": "%recipient\n%organization\n%address\n%postal_code %locality",
    "required_fields": [
        "address"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "AD[1-7]0\\d"
}