{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%locality, %administrative_area\n%postal_code",
    "required_fields": [
        "address",
        "locality",
        "administrative_area"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "province",
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{5}"
}