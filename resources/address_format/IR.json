{
    "locale": "und",
    "format": "%organization\n%recipient\n%administrative_area\n%locality, %dependent_locality\n%address\n%postal_code",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "province",
    "locality_type": "city",
    "dependent_locality_type": "neighborhood",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{5}-?\\d{5}"
}