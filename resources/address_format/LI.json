{
    "locale": "und",
    "format": "%organization\n%recipient\n%address\nFL-%postal_code %locality",
    "required_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "(948[5-9])|(949[0-7])",
    "postal_code_prefix": "FL-"
}