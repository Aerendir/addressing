{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%sorting_code\n%locality\nGUERNSEY\n%postal_code",
    "required_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality",
        "postal_code"
    ],
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "GY\\d[\\dA-Z]?[ ]?\\d[ABD-HJLN-UW-Z]{2}"
}