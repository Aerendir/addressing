{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%locality %administrative_area %postal_code",
    "required_fields": [
        "address",
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "uppercase_fields": [
        "address",
        "locality",
        "recipient",
        "organization",
        "administrative_area"
    ],
    "administrative_area_type": "state",
    "locality_type": "city",
    "postal_code_type": "zip",
    "postal_code_pattern": "(969(?:[12]\\d|3[12]))(?:[ \\-](\\d{4}))?"
}