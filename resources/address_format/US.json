{
    "locale": "en",
    "format": "%recipient\n%organization\n%address\n%locality, %administrative_area %postal_code",
    "required_fields": [
        "address",
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality",
        "administrative_area"
    ],
    "administrative_area_type": "state",
    "postal_code_type": "zip",
    "postal_code_pattern": "(\\d{5})(?:[ \\-](\\d{4}))?"
}