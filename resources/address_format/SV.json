{
    "locale": "es",
    "format": "%recipient\n%organization\n%address\n%postal_code-%locality\n%administrative_area",
    "required_fields": [
        "address",
        "locality",
        "administrative_area"
    ],
    "uppercase_fields": [
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "administrative_area_type": "province",
    "locality_type": "city",
    "postal_code_type": "postal",
    "postal_code_pattern": "CP [1-3][1-7][0-2]\\d"
}