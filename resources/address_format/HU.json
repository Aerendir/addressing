{
    "locale": "und",
    "format": "%recipient\n%organization\n%locality\n%address\n%postal_code",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "address",
        "locality",
        "recipient",
        "organization"
    ],
    "administrative_area_type": "province",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{4}"
}