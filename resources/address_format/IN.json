{
    "locale": "en",
    "format": "%recipient\n%organization\n%address\n%locality %postal_code\n%administrative_area",
    "required_fields": [
        "address",
        "locality",
        "administrative_area",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "administrative_area_type": "state",
    "postal_code_type": "pin",
    "postal_code_pattern": "\\d{6}"
}