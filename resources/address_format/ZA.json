{
    "locale": "und",
    "format": "%recipient\n%organization\n%address\n%dependent_locality\n%locality\n%postal_code",
    "required_fields": [
        "address",
        "locality",
        "postal_code"
    ],
    "uppercase_fields": [
        "locality"
    ],
    "locality_type": "city",
    "dependent_locality_type": "suburb",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{4}"
}