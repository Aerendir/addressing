{
    "locale": "es",
    "format": "%recipient\n%organization\n%address\n%postal_code\n%locality, %administrative_area",
    "required_fields": [
        "address",
        "locality"
    ],
    "uppercase_fields": [
        "locality",
        "administrative_area"
    ],
    "administrative_area_type": "department",
    "postal_code_type": "postal",
    "postal_code_pattern": "\\d{5}"
}